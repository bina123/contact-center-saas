{
    "name": "contact-center/api",
    "type": "project",
    "description": "Multi-tenant Contact Center SaaS API",
    "keywords": ["laravel", "contact-center", "saas", "multi-tenant"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^10.48",
        "laravel/sanctum": "^3.3",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [],
        "post-update-cmd": [],
        "post-root-package-install": [],
        "post-create-project-cmd": []
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
